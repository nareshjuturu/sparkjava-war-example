pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                git branch: 'main', credentialsId: 'github', url: 'git@github.com:nareshjuturu/sparkjava-war-example.git'
            }
        }
        stage("Build") {
            steps {
                sh 'mvn clean  install'
            }
       }
        
    }
}
